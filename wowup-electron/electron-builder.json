{
  "productName": "WowUp",
  "directories": {
    "output": "release/"
  },
  "nodeGypRebuild": true,
  "files": [
    "**/*",
    "build/Release/*.node",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/app/",
    "!src/assets/",
    "!src/environments/",
    "!src/**/*.map",
    "!src/**/*.scss",
    "!src/**/*.html",
    "!src/**/*.conf.js",
    "!src/**/*.json",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json",
    "!.vscode/",
    "!node_modules/@angular"
  ],
  "win": {
    "icon": "dist/assets/icons",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/assets/icons",
    "category": "public.app-category.games",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}
